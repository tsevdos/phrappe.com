{
  "name": "astro-phrappe",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 npm run check && astro build",
    "check": "tsc --noEmit && npm run lint && npm run prettier && NODE_OPTIONS=--max_old_space_size=4096 astro check",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint --ext .ts,.tsx,.astro src",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/mdx": "^2.2.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "astro": "^4.5.8",
    "astro-integration-lottie": "^0.2.3",
    "lottie-web": "^5.12.2",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "typescript": "^5.4.3"
  }
}
